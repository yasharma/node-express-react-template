{
  "name": "currency-conversion",
  "version": "0.0.1",
  "scripts": {
    "stagingstart": "export NODE_ENV=staging && npm run start",
    "teststart": "export NODE_ENV=test && npm run start",
    "dev": "./node_modules/.bin/nodemon --legacy-watch -L -e .ts -w src --exec \"./node_modules/ts-node/dist/bin.js src\"",
    "build": "./node_modules/.bin/tsc --p tsconfig.json",
    "start": "node dist",
    "prestart": "npm install",
    "test": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha 'test/**/*.spec.ts'",
    "lint": "tslint src/*/*.ts --format codeframe",
    "fix-lint": "tslint src/*/*.ts --fix",
    "esw": "esw src/*/*.ts --format codeframe --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "bson": "^3.0.0",
    "continuation-local-storage": "^3.2.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-proxy": "^1.17.0",
    "lodash": "^4.17.15",
    "request": "^2.85.0",
    "semver": "^6.3.0",
    "validatorjs": "^3.14.2"
  },
  "devDependencies": {
    "@types/assert": "^0.0.31",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/express": "^4.16.0",
    "@types/http-proxy": "^1.16.2",
    "@types/lodash": "^4.14.111",
    "@types/mocha": "^5.2.3",
    "@types/mongoose": "^5.0.16",
    "@types/node": "^10.3.4",
    "@types/request": "^2.47.1",
    "@types/sinon": "5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "@types/validatorjs": "^3.7.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^4.0.1",
    "nock": "^9.4.2",
    "nodemon": "^1.12.5",
    "nyc": "^14.1.1",
    "prettier": "1.13.4",
    "proxyquire": "^1.8.0",
    "reqres": "^2.1.0",
    "simple-mock": "^0.8.0",
    "sinon": "6.1.3",
    "sinon-chai": "^2.14.0",
    "source-map-support": "0.5.6",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
